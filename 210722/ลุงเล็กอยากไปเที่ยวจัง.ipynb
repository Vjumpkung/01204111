   "source": [
    "N = int(input(\"N = \"))\r\n",
    "c = []\r\n",
    "f = []\r\n",
    "for i in range(N):\r\n",
    "    c.append(int(input((f\"cost of day {i+1} = \"))))\r\n",
    "i = 0\r\n",
    "while True:\r\n",
    "    try:\r\n",
    "        f.append(c[i]+c[i+1]+c[i+2])\r\n",
    "        i = i + 1\r\n",
    "    except IndexError:\r\n",
    "        break\r\n",
    "print(f\"Min cost of 3 days = {min(f)}\")"
   ],
