    "import json\r\n",
    "fn = input(\"Filename : \")\r\n",
    "df = json.load(open(fn))\r\n",
    "which = input()\r\n",
    "x = [(i['Brand'],i['Cost']) for i in df if which in i.values()]\r\n",
    "filters = min(x,key=lambda x : int(x[1]))\r\n",
    "print(f\"{filters[0]} : {filters[1]}\")"
